<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--注册页面-->
    <style name="style_linearlayout_login_register">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/dp_40</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:paddingLeft">@dimen/dp_10</item>
        <item name="android:paddingRight">@dimen/dp_10</item>
        <item name="android:layout_marginLeft">@dimen/dp_40</item>
        <item name="android:layout_marginRight">@dimen/dp_40</item>
    </style>




    <style name="style_til">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginStart">@dimen/dp_20</item>
        <item name="android:layout_marginEnd">@dimen/dp_20</item>
        <item name="android:focusable">false</item>
        <item name="android:focusableInTouchMode">false</item>
        <item name="hintTextAppearance">@style/hintAppearence</item>

    </style>




</resources>