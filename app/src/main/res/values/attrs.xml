<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="HomeTabView">
        <attr name="normalIcon" format="reference" />
        <attr name="selectIcon" format="reference" />
        <attr name="text" format="string" />
        <attr name="isSelected" format="boolean" />
        <attr name="textNormalColor" format="reference|color" />
        <attr name="textSelectColor" format="reference|color" />
    </declare-styleable>

    <declare-styleable name="SettingItemView">
        <attr name="leftIcon" format="reference" />
        <attr name="showLeftIcon" format="boolean" />
        <attr name="showRightIcon" format="boolean" />
        <attr name="showSubName" format="boolean" />
        <attr name="showRedPoint" format="boolean" />
        <attr name="textSubNameColor" format="reference|color" />
        <attr name="itemName" format="string" />
        <attr name="subName" format="string" />
    </declare-styleable>


    <declare-styleable name="CheckItemView">
        <attr name="checkTitle" format="string" />
    </declare-styleable>


    <declare-styleable name="PersonalView">
        <attr name="personinfo" format="string" />
    </declare-styleable>


    <declare-styleable name="EditTextComposeView">
        <!--左边图标-->
        <attr name="compose_left_icon" format="reference" />
        <!--内容提示-->
        <attr name="compose_hide" format="string|reference" />


        <!--右边图标 文字-->
        <attr name="compose_right_show" format="boolean" />
        <attr name="compose_rigth_icon" format="reference" />
        <attr name="compose_rigth_text" format="reference|string" />
        <attr name="compose_right_type" format="enum" >
            <enum name="right_type_text" value="1"/>
            <enum name="right_type_icon" value="2"/>
        </attr>

        <!--是否密码-->
        <attr name="compose_item_ispassword" format="boolean"/>


        <attr name="compose_content_type" format="enum" >
            <enum name="content_input" value="1"/>
            <enum name="content_text" value="2"/>
        </attr>


    </declare-styleable>


    <declare-styleable name="ToolbarLayout">
        <attr name="toolbarLeftIcon" format="reference" />
        <attr name="toolbarTitle" format="string" />
        <attr name="toolbarRightIcon" format="reference" />
    </declare-styleable>


    <declare-styleable name="TitleBarLayout">
        <attr name="titleText" format="string" />
        <attr name="showLeftBackIcon" format="boolean" />
        <attr name="showRightButton" format="boolean" />
        <attr name="showRightText" format="boolean" />
        <attr name="showRightImage" format="boolean" />
        <attr name="showFilterIcon" format="boolean" />
        <attr name="rightText" format="string" />
        <attr name="rightButtonText" format="string" />
    </declare-styleable>

    <declare-styleable name="DataItemView">
        <attr name="lefticon" format="reference" />
        <attr name="title" format="string" />
        <attr name="value" format="string" />

    </declare-styleable>


    <declare-styleable name="Tab">
        <attr name="tabText" format="string" />
    </declare-styleable>

    <declare-styleable name="CropImageView">
        <attr name="cropGuidelines">
            <enum name="off" value="0" />
            <enum name="onTouch" value="1" />
            <enum name="on" value="2" />
        </attr>
        <attr name="cropScaleType">
            <enum name="fitCenter" value="0" />
            <enum name="center" value="1" />
            <enum name="centerCrop" value="2" />
            <enum name="centerInside" value="3" />
        </attr>
        <attr name="cropShape">
            <enum name="rectangle" value="0" />
            <enum name="oval" value="1" />
        </attr>
        <attr name="cropAutoZoomEnabled" format="boolean" />
        <attr name="cropMaxZoom" format="integer" />
        <attr name="cropMultiTouchEnabled" format="boolean" />
        <attr name="cropFixAspectRatio" format="boolean" />
        <attr name="cropAspectRatioX" format="integer" />
        <attr name="cropAspectRatioY" format="integer" />
        <attr name="cropInitialCropWindowPaddingRatio" format="float" />
        <attr name="cropBorderLineThickness" format="dimension" />
        <attr name="cropBorderLineColor" format="color" />
        <attr name="cropBorderCornerThickness" format="dimension" />
        <attr name="cropBorderCornerOffset" format="dimension" />
        <attr name="cropBorderCornerLength" format="dimension" />
        <attr name="cropBorderCornerColor" format="color" />
        <attr name="cropGuidelinesThickness" format="dimension" />
        <attr name="cropGuidelinesColor" format="color" />
        <attr name="cropBackgroundColor" format="color" />
        <attr name="cropSnapRadius" format="dimension" />
        <attr name="cropTouchRadius" format="dimension" />
        <attr name="cropShowCropOverlay" format="boolean" />
        <attr name="cropShowProgressBar" format="boolean" />
        <attr name="cropMinCropWindowWidth" format="dimension" />
        <attr name="cropMinCropWindowHeight" format="dimension" />
        <attr name="cropMinCropResultWidthPX" format="float" />
        <attr name="cropMinCropResultHeightPX" format="float" />
        <attr name="cropMaxCropResultWidthPX" format="float" />
        <attr name="cropMaxCropResultHeightPX" format="float" />
    </declare-styleable>
</resources>